=\"wifi\">\r\n                <img v-if=\"isColorBlack\" src=\"../assets/wifi.png\" alt=\"...\">\r\n                <img v-else src=\"../assets/wifi-black.png\" alt=\"...\">\r\n            </div>\r\n        </div>\r\n        \r\n        <div class=\"lockicon\" v-if=\"isLocked\">\r\n            <img src=\"../assets/lock.png\" alt=\"...\">\r\n        </div>\r\n        <div class=\"time\" v-else>\r\n            {{hour}}:{{minute}}\r\n        </div>\r\n        <div class=\"power\">\r\n            <Icon type=\"ios-battery-full\" color=\"rgb(33, 248, 97)\" size=\"20\"/>\r\n        </div>\r\n    </div>\r\n    <div class=\"top blackscreened\" v-else></div>\r\n</template>\r\n\r\n<script>\r\nimport {mapGetters} from 'vuex'\r\n\r\nexport default {\r\n  name: 'top',\r\n  props: {\r\n     isBlack: Boolean\r\n  },\r\n  created() {\r\n      this.date = new Date();\r\n      //console.log(\"created\");\r\n  },\r\n  mounted() {\r\n    this.updateTimeInterval = this.updateTime();\r\n  },\r\n  methods: {\r\n      //update time \r\n      updateTime() {\r\n          return setInterval(() => {\r\n              this.date = new Date();\r\n          }, 500);\r\n      },\r\n  },\r\n  data() {\r\n      return{\r\n          //current date\r\n          date: null,\r\n          //update time interval\r\n          updateTimeInterval: null,\r\n          //true when the main color if black\r\n          isColorBlack: this.isBlack\r\n      }\r\n  },\r\n  computed: {\r\n      ...mapGetters([\r\n          'isPoweroffConfirmed',\r\n          'isPoweredOff',\r\n          'isLocked',\r\n          'isBlackScreen'\r\n      ]),\r\n      hour() {\r\n          return this.date.getHours() < 10 ? '0' + this.date.getHours() : this.date.getHours();\r\n      },\r\n      minute() {\r\n          return this.date.getMinutes() < 10 ? '0' + this.date.getMinutes() : this.date.getMinutes();\r\n      },\r\n  },\r\n  watch: {\r\n      isBlack() {\r\n          this.isColorBlack = this.isBlack;\r\n          console.log(this.isBlack);\r\n      }\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style lang=\"scss\" scoped>\r\n.top {\r\n    width: 100%;\r\n    height: 5%;\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    flex-wrap: nowrap;\r\n    img {\r\n        height: 10px;\r\n        width: 10px;\r\n    }\r\n    .top-left {\r\n        width: 30%;\r\n        display: flex;\r\n        justify-content: space-around;\r\n        align-items: center;\r\n\r\n        .runner {\r\n            font-size: 13px;\r\n        }\r\n        .wifi {\r\n            img {\r\n                height: 18px;\r\n                width: 18px;\r\n                display: flex;\r\n            }\r\n        }\r\n    }\r\n    .lockicon {\r\n        width: 40%;\r\n    }\r\n    .time {\r\n        width: 40%;\r\n        text-align: center;\r\n    }\r\n    .power {\r\n        width: 30%;\r\n        display: flex;\r\n        justify-content: flex-end;\r\n    }\r\n}  \r\n\r\n.blackscreened {\r\n    background-color: #000;\r\n}\r\n</style>\r\n"]}]}